<?xml version="1.0" ?><!-- Simple 4 wheeled tank drive Rover with Lidar Sensor and Camera Model -->
<robot name="rover">

    <!-- Base Link -->
  <link name="chassis">
    <visual>
      <geometry>
          <box size=".8 0.7 0.1"/>
      </geometry>
      <material name="gray">
        <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  <inertial>
            <mass value="2" />
            <inertia ixx="0.04" ixy="0.0" ixz="0" 
            iyy="0.17" iyz="0" izz="0.25" />
    </inertial>
    
    <collision>
      <geometry>
          <box size=".8 0.7 0.1"/>
      </geometry>
    </collision>
  </link>


  <link name="wheel_front_right">
    <inertial>
            <mass value="2" />
            <inertia ixx="1e-3" ixy="0.0" ixz="0" 
            iyy="1e-3" iyz="0" izz="1e-3" />
    </inertial>

    <visual>
      
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
    
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_front_right_joint" type="continuous">
    <origin xyz="0.25 0.4 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="wheel_front_right"/>
    <axis xyz="0.0 0.0 1.0"/>
  </joint>

  <link name="wheel_front_left">
    <inertial>
            <mass value="2" />
            <inertia ixx="1e-3" ixy="0.0" ixz="0" 
            iyy="1e-3" iyz="0" izz="1e-3" />
    </inertial>

    <visual>
      
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_front_left_joint" type="continuous">
    <origin xyz="0.25 -0.4 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="wheel_front_left"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

<link name="wheel_back_right">
    <inertial>
            <mass value="2" />
            <inertia ixx="1e-3" ixy="0.0" ixz="0" 
            iyy="1e-3" iyz="0" izz="1e-3" />
    </inertial>

    <visual>
      
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_back_right_joint" type="continuous">
    <origin xyz="-0.25 0.4 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="wheel_back_right"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

  <link name="wheel_back_left">
    <inertial>
            <mass value="2" />
            <inertia ixx="1e-3" ixy="0.0" ixz="0" 
            iyy="1e-3" iyz="0" izz="1e-3" />
    </inertial>

    <visual>
      
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <contact_coefficients mu="1" kp="1e+13" kd="1.0"/>
    </collision>
  </link>

  <joint name="wheel_back_left_joint" type="continuous">
    <origin xyz="-0.25 -0.4 0.0" rpy="1.57 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="wheel_back_left"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

  <link name="camera">
    <inertial>
            <mass value="0.5" />
            <inertia ixx="0.01" ixy="0.0" ixz="0" 
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      
      <geometry>
        <box size="0.1 0.5 0.2"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
          <box size="0.1 0.5 0.2"/>
      </geometry>
    </collision>
  </link>

  <joint name="camera_joint" type="fixed">
    <origin xyz="-0.35 0 0.15" rpy="0 0.0 3.14"/>
    <parent link="chassis"/>
    <child link="camera"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>

  <link name="lidar">
    <inertial>
            <mass value="0.5" />
            <inertia ixx="0.01" ixy="0.0" ixz="0" 
            iyy="0.01" iyz="0" izz="0.01" />
    </inertial>

    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0  1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
    </collision>
  </link>

  <joint name="lidar_joint" type="fixed">
    <origin xyz="-0.43 0 0.0" rpy="0 0.0 1.57"/>
    <parent link="chassis"/>
    <child link="lidar"/>
    <axis xyz="0.0 0.0 1.0"/>

  </joint>